<div class="container">
	<div style="margin-top: 50px">
	<a href="<%=new_itinerary_traveler_path%>" class="btn btn-primary pull-left">Add a Traveler</a>
	<a href="<%=new_itinerary_waypoint_path%>" class="btn btn-primary pull-left" style="margin-left:20px">Add a Waypoint</a>
	<a href="<%= itineraries_path%>" class="btn btn-primary pull-right">Back to Itineraries</a>
	</div>
	<br>
	<hr/>
	<% @itinerary.travelers.each do |t| %>
		<div style="margin-top: 10px">
			<h3>Name: <small><%= t.first_name %> <%= t.last_name %></small> Email: <small><%= t.email %></small></h3>
			<a href="<%= edit_itinerary_traveler_path(@itinerary.id, t.id)%>" class="btn btn-primary pull-left">Edit Traveler</a>
		</div>
		<br>
		<hr>
	<%end %>
</div>
